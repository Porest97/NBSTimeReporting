@model IEnumerable<NBSTimeReporting.DWorkin.Regus.DataModels.RegusTicket>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a class="btn btn-sm btn-success" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TicketPriority)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TicketStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TicketType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RegusTicketNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IncidentNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Creator)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Site)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Received)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Receiver)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FEScheduled)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FEAssigned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FEEntersSite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FEEExitsSite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Logg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IssueResolved)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Resolution)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WLNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WLHours)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TicketPriority.TicketPriorityName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TicketStatus.TicketStatusName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TicketType.TicketTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegusTicketNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncidentNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Creator.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Site.SiteName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Received)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Receiver.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FEScheduled)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FEAssigned.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FEEntersSite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FEEExitsSite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Logg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IssueResolved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resolution)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WLNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WLHours)
            </td>
            <td>
                <a class="btn btn-sm btn-success" Edit" asp-route-id="@item.Id">Edit</a>
                <a class="btn btn-sm btn-success" asp-action="Details" asp-route-id="@item.Id">Details</a>
                <a class="btn btn-sm btn-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                <a class="btn tbn-sm btn-success" asp-action="ListRegusTickets">Back to Regus tickets</a>
            </td>
        </tr>
}
    </tbody>
</table>
